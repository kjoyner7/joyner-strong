<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.tmd.dao.data.TmdEmployeeStatus" table="tmd_employee_status">
        <id name="tmdEmployeeStatusUid" type="java.lang.Integer">
            <column name="tmd_employee_status_uid" />
            <generator class="identity" />
        </id>
        <property name="vtigerUsersId" type="java.lang.Integer">
            <column name="vtiger_users_id" />
        </property>
        <property name="employeeStatus" type="java.lang.Integer">
        	<column name="employee_status" />
        </property>
        <property name="dateAdded" type="timestamp">
            <column name="date_added" length="19"  />
        </property>
        <property name="dateModified" type="timestamp">
            <column name="date_modified" length="19"/>
        </property>
    </class>
</hibernate-mapping>